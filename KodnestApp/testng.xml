<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="SuiteKodnest">

 	<test name="TestPasswordRecoveryKodNest" thread-count="5">
     <parameter name="passwordrecoveryurl" value="https://app.kodnest.com/forgot-password"></parameter>
    <classes>
    <class name="com.Kodnest.ForgotPassword.ForgotPassword"></class>
    </classes>
    </test>
    
    <test name="TestRegistrationKodNest" thread-count="5">
    <parameter name="registrationurl" value="https://www.kodnest.com/"></parameter>
    <classes>
    <class name="com.Kodnest.Registration.RegistrationTestCases"></class>
    </classes>
    </test>
    
     <test name="TestLoginKodNest" thread-count="5">
     <parameter name="loginurl" value="https://app.kodnest.com/login"></parameter>
    <classes>
    <class name="com.Kodnest.Login.LoginPage"></class>
    </classes>
    </test>
</suite>
